{
    "family": "communiteam-server",
    "executionRoleArn": "arn:aws:iam::007555096130:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "communiteam-server",
            "image": "007555096130.dkr.ecr.us-east-2.amazonaws.com/communiteam-server:latest",
            "environmentFiles": [
                {
                  "value": "arn:aws:s3:::communiteam-ci-cd/.env",
                  "type": "s3"
                }],
            "portMappings": [
                {
                    "containerPort": 5000,
                    "hostPort": 8888,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "128"
}